{
  "name": "aqsc_ui",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "build": "webpack --config webpack.dev.js",
    "build:watch": "webpack --config webpack.dev.js --watch",
    "build:prod": "webpack --config webpack.prod.js",
    "start": "sirv dist --single --cors --host 0.0.0.0  --port 3000",
    "start:proxy": "node ./proxy_server/app.js",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "type-check": "tsc --noEmit",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "react-router-dom": "^6.6.2",
    "rimraf": "^4.1.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@babel/types": "^7.21.4",
    "@msgpack/msgpack": "^2.8.0",
    "@patternfly/react-charts": "^6.94.15",
    "@patternfly/react-core": "^4.267.6",
    "@patternfly/react-icons": "^4.93.3",
    "@patternfly/react-styles": "^4.92.3",
    "@patternfly/react-table": "^4.113.7",
    "axios": "^1.4.0",
    "base-64": "^1.0.0",
    "core-util-is": "^1.0.3",
    "postcss-loader": "^7.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^13.2.2",
    "sirv-cli": "^2.0.2",
    "ws": "^8.13.0"
  }
}
